<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      android:versionCode="1"
      android:versionName="1.0" package="com.example.usbfastdemo">
    <application android:icon="@drawable/waterloo" android:label="@string/app_name" android:debuggable="true">
        <activity android:name="USBFastActivity" android:label="@string/app_name" android:screenOrientation="landscape">
            <intent-filter>
                <!-- <action android:name="com.example.usbfastdemo.USBFastCV" /> -->
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
    <!-- <uses-sdk android:minSdkVersion="8"  android:targetSdkVersion="8"/> -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- Tell the system this app requires OpenGL ES 2.0. -->
    <uses-feature android:glEsVersion="0x00020000" android:required="true" />
</manifest> 
